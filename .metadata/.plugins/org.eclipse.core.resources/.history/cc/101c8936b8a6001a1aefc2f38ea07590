package com.tsarhydrantine.tsbase.tsEntity.DeadMau5;

import com.tsarhydrantine.tsbase.TsRegistry;

import net.minecraftforge.client.event.RenderPlayerEvent;
import net.minecraftforge.eventbus.api.SubscribeEvent;

public class AddPlayerAttribute {
	@SubscribeEvent
	public void onPlayerRender(RenderPlayerEvent.Pre event) {
		event.getRenderer().addLayer(new DeadMau5Ears(event.getRenderer()));
		TsRegistry.logger.info("onPlayerRender called....");
	}
}
